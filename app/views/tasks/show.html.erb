<%- model_class = Task -%>

<div class="page-header">
  <h3>
    <%= image_tag @customer.logo.url(:small) %>
    <%= link_to "#{@project.name} ", customer_project_path(@customer, @project), class: 'small' %> \ <%= @task.title %>
  </h3>
</div>

<p id="notice"><%= notice %></p>

<%= link_to "Novo Item", new_customer_project_task_item_path(@customer, @project, @task), class: 'btn btn-primary' %>
<%= render :partial => 'items/list' %>
